<div class="row"> 
	<div class="col-md-12">
		<div class="panel panel-inverse" data-sortable-id="form-stuff-4">
			<div class="panel-heading">
				<h4 class="panel-title">Add new Member</h4>
			</div>
			<div class="panel-body">
				<form id="register" action="/newMember" method="post" class="form-horizontal">
					<fieldset>
						<div class="form-group">
							<label class="col-md-2 control-label">First Name</label>
							<div class="col-md-10">
								<input id="given-name" name="name" type="text" placeholder="First name" class="form-control" required autofocus>
							</div>
						</div>
						<div class="form-group">
							<label class="col-md-2 control-label">Last Name</label>
							<div class="col-md-10">
								<input id="family-name" name="sn" type="text" placeholder="Last name only" class="form-control" required autofocus>
							</div>
						</div>
						<div class="form-group">
							<label class="col-md-2 control-label">Email Address</label>
							<div class="col-md-10">
								<input id="email" name="mail" type="email" placeholder="example@domain.com" class="form-control"required>
							</div>
						</div>
						<div class="form-group">
							<label class="col-md-2 control-label">Membership From</label>
							<div class="col-md-10">
								<input id="memberSinceDate" name="memberSinceDate" type="text" placeholder="format should be YYYYMMDD" class="form-control" required>
							</div>
						</div>
						<div class="form-group">
							<label class="col-md-2 control-label">Membership Until</label>
							<div class="col-md-10">
								<input id="memberUntilDate" name="memberUntilDate" type="text" placeholder="format should be YYYYMMDD" class="form-control" required>
							</div>
						</div>
						<div class="form-group">
							<div class="col-md-10 col-md-offset-2">
								<button type="submit" class="btn btn-sm btn-primary m-r-5">Submit</button>
							</div>
						</div>
					</fieldset>
				</form>
			</div>
		</div>
	</div>
</div> <!-- /row end -->
